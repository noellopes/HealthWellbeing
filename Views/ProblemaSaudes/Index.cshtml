            <td>
                @Html.DisplayFor(modelItem => item.ProblemaCategoria)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.ProblemaNome)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.ZonaAtingida)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.ProfissionalDeApoio)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.Gravidade)
            </td>
            <td>
