@model IEnumerable<HealthWellbeing.Models.ProblemaSaude>

@{
    ViewData["Title"] = "Index";
}

<h1>Index</h1>

<p>
    <a asp-action="Create">Create New</a>
</p>
<table class="table">
    <thead>
        <tr>
            <th>
                @Html.DisplayNameFor(model => model.ProblemaCategoria)
            </th>
            <th>
                @Html.DisplayNameFor(model => model.ProblemaNome)
            </th>
            <th>
                @Html.DisplayNameFor(model => model.ZonaAtingida)
            </th>
