@model IEnumerable<HealthWellbeing.Models.ProfissionalExecutante>

@{
    ViewData["Title"] = "Profissionais Executantes Page";
}

<h1>Viewbag.Title</h1>

<p>
    <a asp-action="Create">Create New</a>
</p>
<table class="table">
    <thead>
        <tr>
            <th>
                @Html.DisplayNameFor(model => model.Nome)
            </th>
            <th>
                @Html.DisplayNameFor(model => model.Funcao)
            </th>
            <th>
                @Html.DisplayNameFor(model => model.Telefone)
            </th>
            <th>
                @Html.DisplayNameFor(model => model.Email)
            </th>
            <th></th>
        </tr>
    </thead>
    <tbody>
@foreach (var item in Model) {
        <tr>
            <td>
                @Html.DisplayFor(modelItem => item.Nome)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.Funcao)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.Telefone)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.Email)
            </td>
            <td>
                <a asp-action="Edit" asp-route-id="@item.ProfissionalExecutanteId">Edit</a> |
                <a asp-action="Details" asp-route-id="@item.ProfissionalExecutanteId">Details</a> |
                <a asp-action="Delete" asp-route-id="@item.ProfissionalExecutanteId">Delete</a>
            </td>
        </tr>
}
    </tbody>
</table>
