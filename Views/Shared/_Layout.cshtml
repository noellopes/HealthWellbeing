<!DOCTYPE html>
<html lang="en">
<head>
    <meta charset="utf-8" />
    <meta name="viewport" content="width=device-width, initial-scale=1.0" />
    <title>@ViewData["Title"] - HealthWellbeing</title>
    <link rel="stylesheet" href="~/lib/bootstrap/dist/css/bootstrap.min.css" />
    <link rel="stylesheet" href="~/css/site.css" asp-append-version="true" />
    <link rel="stylesheet" href="~/HealthWellbeing.styles.css" asp-append-version="true" />
</head>
<body>
    <header>
        <nav class="navbar navbar-expand-sm navbar-toggleable-sm navbar-light bg-white border-bottom box-shadow mb-3">
            <div class="container-fluid">
                <a class="navbar-brand" asp-area="" asp-controller="Home" asp-action="Index">HealthWellbeing</a>
                <button class="navbar-toggler" type="button" data-bs-toggle="collapse" data-bs-target=".navbar-collapse" aria-controls="navbarSupportedContent"
                        aria-expanded="false" aria-label="Toggle navigation">
                    <span class="navbar-toggler-icon"></span>
                </button>
                <div class="navbar-collapse collapse d-sm-inline-flex justify-content-between">
                    <ul class="navbar-nav flex-grow-1">
                        <li class="nav-item">
                            <a class="nav-link text-dark" asp-area="" asp-controller="Home" asp-action="Index">Home</a>
                        </li>
                        <li class="nav-item">
                            <a class="nav-link text-dark" asp-area="" asp-controller="Home" asp-action="Privacy">Privacy</a>
                        </li>
                        <li class="nav-item">
                            <a class="nav-link text-dark" asp-area="" asp-controller="Alimento" asp-action="Index">Alimento</a>
                        </li>
                        <li class="nav-item">
                            <a class="nav-link text-dark" asp-area="" asp-controller="ComponentesAlimento" asp-action="Index">ComponentesAlimento</a>
                        </li>
                        <li class="nav-item dropdown">
                            <a class="nav-link dropdown-toggle text-dark" href="#" id="navbarDropdown" role="button" data-bs-toggle="dropdown" aria-expanded="false">
                                Grupo 7
                            </a>
                            <ul class="dropdown-menu" aria-labelledby="navbarDropdown">
                              
                                <li>
                                    <a class="dropdown-item" asp-area="" asp-controller="TipoExercicio" asp-action="Index">
                                        Tipos de Exercício
                                    </a>
                                </li>
                                <li>
                                    <a class="dropdown-item" asp-area="" asp-controller="Beneficio" asp-action="Index">
                                        Benefícios
                                    </a>
                                </li>
                                
                              
                                <li>
                                    <a class="dropdown-item" asp-area="" asp-controller="Exercicios" asp-action="Index">
                                        Exercícios
                                    </a>
                                </li>
                                <li>
                                    <a class="dropdown-item" asp-area="" asp-controller="Genero" asp-action="Index">
                                        Gêneros
                                    </a>
                                </li>
                              
                                <li>
                                    <a class="dropdown-item" asp-area="" asp-controller="GrupoMuscular" asp-action="Index">
                                        Grupos Musculares
                                    </a>
                                </li>
                                <li>
                                    <a class="dropdown-item" asp-area="" asp-controller="Musculo" asp-action="Index">
                                        Músculos
                                    </a>
                                </li>
                               
                                <li>
                                    <a class="dropdown-item" asp-area="" asp-controller="ProblemaSaudes" asp-action="Index">
                                        Problemas de Saúde
                                    </a>
                                </li>
                                <li>
                                    <a class="dropdown-item" asp-controller="AvaliacaoFisica" asp-action="Index">
                                        Avaliação Física
                                    </a>
                                </li>
                                <li>
                                    <a class="dropdown-item" asp-controller="Beneficio" asp-action="Index">
                                        Benefícios
                                    </a>
                                </li>
                                <li>
                                    <a class="dropdown-item" asp-controller="Equipamento" asp-action="Index">
                                        Equipamentos
                                    </a>
                                </li>
                                <li>
                                    <a class="dropdown-item" asp-controller="Estatisticas" asp-action="Index">
                                        Estatísticas
                                    </a>
                                </li>
                                <li>
                                    <a class="dropdown-item" asp-controller="Genero" asp-action="Index">
                                        Géneros
                                    </a>
                                </li>
                                <li>
                                    <a class="dropdown-item" asp-controller="HistoricoAtividades" asp-action="Index">
                                        Histórico Atividades
                                    </a>
                                </li>
                                <li>
                                    <a class="dropdown-item" asp-controller="ObjetivoFisico" asp-action="Index">
                                        Objetivos Físicos
                                    </a>
                                </li>
                                <li>
                                    <a class="dropdown-item" asp-controller="PlanoExercicios" asp-action="Index">
                                        Plano de Exercícios
                                    </a>
                                </li>
                                <li>
                                    <a class="dropdown-item" asp-controller="Sono" asp-action="Index">
                                        Sono
                                    </a>
                                </li>
                                <li>
                                    <a class="dropdown-item" asp-controller="Users" asp-action="Index">
                                        Users (Gestão)
                                    </a>
                                </li>
                                <li>
                                    <a class="dropdown-item" asp-controller="UtenteGrupo7" asp-action="Index">
                                        Utentes
                                    </a>
                                </li>
                            </ul>
                        </li>
                    </ul>
                    <partial name="_LoginPartial" />
                </div>
            </div>
        </nav>
    </header>
    <div class="container">
        <main role="main" class="pb-3">
            @RenderBody()
        </main>
    </div>

    <footer class="border-top footer text-muted">
        <div class="container">
            &copy; 2025 - HealthWellbeing - <a asp-area="" asp-controller="Home" asp-action="Privacy">Privacy</a>
        </div>
    </footer>
    <script src="~/lib/jquery/dist/jquery.min.js"></script>
    <script src="~/lib/bootstrap/dist/js/bootstrap.bundle.min.js"></script>
    <script src="~/js/site.js" asp-append-version="true"></script>
    @await RenderSectionAsync("Scripts", required: false)
</body>
</html>
