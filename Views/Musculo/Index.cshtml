@model IEnumerable<HealthWellbeing.Models.Musculo>

@{
    ViewData["Title"] = "Index";
}

<h1>Index</h1>

<p>
    <a asp-action="Create">Create New</a>
</p>
<table class="table">
    <thead>
        <tr>
            <th>
                @Html.DisplayNameFor(model => model.Nome_Musculo)
            </th>
            <th>
                @Html.DisplayNameFor(model => model.GrupoMuscular)
            </th>
            <th></th>
        </tr>
    </thead>
    <tbody>
@foreach (var item in Model) {
        <tr>
            <td>
                @Html.DisplayFor(modelItem => item.Nome_Musculo)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.GrupoMuscular.GrupoMuscularNome)
            </td>
            <td>
                <a asp-action="Edit" asp-route-id="@item.MusculoId">Editar</a> |
                <a asp-action="Details" asp-route-id="@item.MusculoId">Detalhes</a> |
                <a asp-action="Delete" asp-route-id="@item.MusculoId">Eliminar</a>
            </td>
        </tr>
}
    </tbody>
</table>
