@model HealthWellbeing.Models.Objective
@{
    ViewData["Title"] = "Objective Details";
}

<h1 class="mb-3">Objective Details</h1>
<hr />

<dl class="row">
    <dt class="col-sm-2">@Html.DisplayNameFor(m => m.Name)</dt>
    <dd class="col-sm-10">@Model.Name</dd>

    <dt class="col-sm-2">@Html.DisplayNameFor(m => m.Category)</dt>
    <dd class="col-sm-10">@Model.Category</dd>

    <dt class="col-sm-2">@Html.DisplayNameFor(m => m.Details)</dt>
    <dd class="col-sm-10">@Model.Details</dd>
</dl>

<p class="mt-3">
    <a class="btn btn-primary" asp-action="Edit" asp-route-id="@Model.ObjectiveId">Edit</a>
    <a class="btn btn-secondary" asp-action="Index">Back</a>
</p>
